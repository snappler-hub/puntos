<header class="content-header">
  <%= link_to 'Nuevo Prestador', new_supplier_path,
      class: 'btn btn-primary pull-right' %>
  <h1>Prestadores</h1>
</header>

<!-- Main content -->
<section class="content">

  <table class="table">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Activo</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <%= render @suppliers %>
    </tbody>
  </table>

</section>